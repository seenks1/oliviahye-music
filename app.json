{
    "name": "Express",
    "description": "A good and functional Discord music and moderation bot in node.js.",
    "repository": "https://github.com/ItzLightyHD/Express",
    "logo": "https://raw.githubusercontent.com/ItzLightyHD/Express/master/logo.png",
    "keywords": ["node", "discord", "youtube"],
    "image": "heroku/nodejs",
    "buildpacks": [
      {
        "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
      },
      {
        "url": "heroku/nodejs"
      }
    ],
    "env": {
      "TOKEN": {
        "description": "The token of your Discord Bot",
        "required": "true"
      },
      "OWNERID": {
        "description": "Your Discord user ID, needed for some commands...",
        "required": "true"
      },
      "BOT_INVITE": {
        "description": "Invite link for the bot",
        "required": "true"
      },
      "PREFIX": {
        "description": "The prefix you want to use with the bot.",
        "value": ";",
        "required": "true"
      }
    }  
  }
